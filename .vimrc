set number
set expandtab
set hlsearch
set ignorecase
set incsearch
set smartcase
set laststatus=2
syntax on
set autoindent
set wildmenu
set backspace=indent,eol,start
set showmatch
set shiftwidth=2
set tabstop=2

inoremap jk <ESC>
inoremap [ []<LEFT>
inoremap ( ()<LEFT>
inoremap { {}<LEFT>
inoremap " ""<LEFT>
inoremap ' ''<LEFT>

